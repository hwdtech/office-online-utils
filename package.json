{
  "name": "office-online-utils",
  "version": "2.3.1",
  "description": "Utility set for easy Office Online integration",
  "main": "index.js",
  "types": "./types",
  "repository": {
    "type": "git",
    "url": "git@github.com:hwdtech/office-online-utils.git"
  },
  "author": "Maxim Malov <mmalov@hwdtech.ru>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": "8.9.x"
  },
  "scripts": {
    "lint": "eslint ./discovery index.js --fix",
    "postlint": "dtslint types",
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "he": "^1.1.1",
    "node-fetch": "^1.7.3"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/node": "^9.4.0",
    "dtslint": "^0.2.0",
    "eslint": "^4.11.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "jest-runner-eslint": "^0.3.0",
    "lint-staged": "^5.0.0",
    "node-mocks-http": "^1.6.6",
    "prettier": "^1.8.2"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "\\.test\\.js$",
    "collectCoverageFrom": ["lib/**/*.js", "!**/node_modules/**"]
  }
}
